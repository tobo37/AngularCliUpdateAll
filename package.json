{
  "name": "update-them-all",
  "version": "1.1.0",
  "description": "A package to update Angular, dependencies, and devDependencies",
  "main": "index.js",
  "files": [
    "*.js",
    "*.d.ts",
    "model/**"
  ],
  "scripts": {
    "test": "jest --coverage",
    "link": "npm link && npm run postinstall",
    "build": "tsc",
    "copy-files": "ncp package.json dist/package.json && ncp README.md dist/README.md",
    "lint": "eslint . --ext .ts",
    "prelink": "npm run build && npm run copy-files",
    "prepublishOnly": "npm run build && npm run copy-files && cd dist && npm pack"
  },
  "type": "commonjs",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [],
    "testTimeout": 1800000
  },
  "bin": {
    "update-them-all": "./cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tobo37/AngularCliUpdateAll.git"
  },
  "keywords": [
    "angular",
    "npm",
    "cli",
    "updater",
    "update",
    "package"
  ],
  "author": "Tobo37",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tobo37/AngularCliUpdateAll/issues"
  },
  "homepage": "https://github.com/tobo37/AngularCliUpdateAll#readme",
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.2",
    "@types/jsonfile": "^6.1.1",
    "@types/node": "^20.3.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.44.0",
    "fs-extra": "^11.1.1",
    "jest": "^29.5.0",
    "ncp": "^2.0.0",
    "semver": "^7.5.3",
    "ts-jest": "^29.1.1",
    "tslint": "^6.1.3",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "chmodr": "^1.2.0",
    "jsonfile": "^6.1.0",
    "kleur": "^4.1.5"
  }
}
