{
  "name": "update-them-all",
  "version": "1.0.2",
  "description": "Angular & Package Updater",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tobo37/AngularCliUpdateAll.git"
  },
  "keywords": [
    "Angular",
    "npm",
    "updater",
    "update all",
    "package.json"
  ],
  "author": "tobo37",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tobo37/AngularCliUpdateAll"
  },
  "homepage": "https://github.com/tobo37/AngularCliUpdateAll#readme",
  "devDependencies": {
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "jest": "^26.2.2",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7"
  },
  "files": [
    "lib/**/*"
  ]
}
